generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Candidatura {
  id           Int      @id @default(autoincrement())
  nome         String
  telefone     String
  cpf          String
  nascimento   DateTime
  vaga         String?
  localizacao  String?
  curriculo    String?
  createdAt    DateTime @default(now())
}
